{
    "name": "dits-sa/laravel-ai-assistant",
    "description": "A dynamic AI assistant package for Laravel applications that automatically discovers schema and provides real-time AI capabilities",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "DITS SA",
            "email": "info@dits-sa.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/validation": "^9.0|^10.0|^11.0",
        "illuminate/auth": "^9.0|^10.0|^11.0",
        "illuminate/cache": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelAIAssistant\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelAIAssistant\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelAIAssistant\\Providers\\AIAssistantServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "homepage": "https://github.com/dits-sa/laravel-ai-assistant",
    "support": {
        "issues": "https://github.com/dits-sa/laravel-ai-assistant/issues",
        "source": "https://github.com/dits-sa/laravel-ai-assistant"
    },
    "keywords": [
        "laravel",
        "ai",
        "assistant",
        "chatbot",
        "openai",
        "dynamic",
        "schema",
        "api",
        "dits-sa"
    ]
}
